import React from 'react'
import {
  Container
  Row
} from 'reactstrap'
import Day from './Day'

export default Week({ menu, id, onAdd }) ->
  <Container className="mt-4">
    <Row className="row-cards row-deck m-3">
      {menu~map((val, idx) ->
        <Day
          {...val}
          className="col-12 col-sm-12 col-lg"
          onAdd={-> onAdd(id, idx)}
          key={idx}
        />
      )}
    </Row>
  </Container>
